{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "extends": "defaults",
    "rules": {
        "quotes": [2, "single"],
        "no-underscore-dangle": 0
    },
    "globals": {
        "React"
    },
    "plugins": [
        "react"
    ]
}
